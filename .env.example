# Server Configuration
PORT=3000
NODE_ENV=production

# Database Configuration
# For Railway: use /app/data/provenance.db with persistent volume
DATABASE_PATH=./data/provenance.db

# Mina Network Configuration
MINA_NETWORK=testnet
MINA_GRAPHQL_ENDPOINT=https://api.minascan.io/node/devnet/v1/graphql

# zkApp Configuration (REQUIRED - Add your own values)
ZKAPP_ADDRESS=your_deployed_zkapp_address_here
FEE_PAYER_PRIVATE_KEY=your_mina_private_key_here
ZKAPP_PRIVATE_KEY=

# Upload Configuration
UPLOAD_MAX_SIZE=10485760

# Proof Generation
PROOF_GENERATION_TIMEOUT=600000
CIRCUIT_CACHE_PATH=./cache

# API Configuration
API_RATE_LIMIT=100
CORS_ORIGIN=*
COMPRESSION_ENABLED=true

# Logging
LOG_LEVEL=info
 
 