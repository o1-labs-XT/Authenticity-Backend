# Copy this to .env.local for local development

# Backend API URL
NEXT_PUBLIC_API_URL=http://localhost:3000

# Admin dashboard authentication (for accessing the dashboard)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=pass

# Backend API authentication (requests made by nextjs server to the back end api)
# Format: username:password for Basic auth or just an API key
BACKEND_API_KEY=admin:serverpass