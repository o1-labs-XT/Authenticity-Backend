{
  "name": "provenance-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev:api": "tsx src/index.ts",
    "dev:worker": "tsx src/worker.ts",
    "build": "tsc",
    "start": "npm run db:migrate && tsx scripts/compile-zkapp.ts && node dist/index.js",
    "start:api": "npm run db:migrate && node dist/index.js",
    "start:worker": "tsx scripts/compile-zkapp.ts && node dist/worker.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "tsx node_modules/.bin/knex migrate:latest",
    "db:rollback": "tsx node_modules/.bin/knex migrate:rollback",
    "db:migrate:make": "tsx node_modules/.bin/knex migrate:make",
    "db:reset": "docker-compose down -v && docker-compose up -d && sleep 3 && npm run db:migrate",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@types/knex": "^0.15.2",
    "@types/pg": "^8.15.5",
    "authenticity-zkapp": "0.1.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "knex": "^3.1.0",
    "multer": "^2.0.2",
    "o1js": "^2.8.0",
    "pg": "^8.16.3",
    "pg-boss": "^10.3.2",
    "pino": "^9.9.4",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/form-data": "^2.2.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "axios": "^1.11.0",
    "eslint": "^9.33.0",
    "form-data": "^4.0.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
