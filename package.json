{
  "name": "provenance-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec tsx src/index.ts",
    "build": "tsc",
    "start": "tsx scripts/compile-zkapp.ts && node dist/index.js",
    "test": "jest --detectOpenHandles",
    "test:unit": "jest --testPathPattern=unit --detectOpenHandles",
    "test:integration": "jest --testPathPattern=integration --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "tsx node_modules/.bin/knex migrate:latest",
    "db:migrate:dev": "tsx node_modules/.bin/knex migrate:latest --env development",
    "db:migrate:prod": "tsx node_modules/.bin/knex migrate:latest --env production",
    "db:rollback": "tsx node_modules/.bin/knex migrate:rollback",
    "db:migrate:make": "tsx node_modules/.bin/knex migrate:make",
    "db:seed": "tsx node_modules/.bin/knex seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@types/knex": "^0.15.2",
    "@types/pg": "^8.15.5",
    "authenticity-zkapp": "0.1.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "knex": "^3.1.0",
    "multer": "^2.0.2",
    "o1js": "^2.8.0",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/form-data": "^2.2.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "axios": "^1.11.0",
    "eslint": "^9.33.0",
    "form-data": "^4.0.4",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
